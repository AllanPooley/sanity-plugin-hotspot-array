    "react": "^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "dev-preview || 3.0.0-rc.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@sanity/image-url": "1.0.1",
    "lodash": "4.17.21",
    "react": "^18.0.0",
    "sanity": "dev-preview || 3.0.0-rc.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-hotspot-array/issues"
