    "@sanity/util": "3.0.0-rc.0",
    "framer-motion": "^6.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/pkg-utils": "^1.16.2",
